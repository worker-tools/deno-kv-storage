{
  "name": "@worker-tools/deno-kv-storage",
  "description": "",
  "version": "0.3.2",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./sqlite": {
      "import": "./dist/sqlite.js"
    }
  },
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "deno run --allow-read --allow-write test/index.ts",
    "prepack": "npm run build",
    "build": "npm run build:mod && npm run build:sqlite",
    "build:mod": "mkdir -p dist && deno bundle mod.ts > dist/index.js",
    "build:sqlite": "mkdir -p dist && deno bundle sqlite.ts > dist/sqlite.js"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "MIT",
  "dependencies": {
    "kv-storage-interface": "^0.2.0"
  }
}
